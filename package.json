{
  "name": "vue2-drag",
  "version": "0.0.2",
  "description": "Vue component for drag and drop",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node build/start.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "dev-webpack": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.dev.js",
    "build-webpack": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.jsx,.vue src",
    "lint-fix": "eslint --fix --ext .js,.jsx,.vue src",
    "commit": "npm install -g commitizen && commitizen init cz-conventional-changelog --save-dev --save-exact"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhao-jun/vue2-drag.git"
  },
  "author": "zhao-jun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhao-jun/vue2-drag/issues"
  },
  "homepage": "https://github.com/zhao-jun/vue2-drag#readme",
  "keywords": [
    "vue",
    "component",
    "drag",
    "drop"
  ],
  "peerDependencies": {
    "vue": "2.x"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mockjs": "^1.0.1-beta3",
    "ora": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "vconsole": "^3.2.0",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn run lint-fix",
      "git add"
    ]
  }
}
